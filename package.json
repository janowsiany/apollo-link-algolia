{
  "name": "apollo-link-algolia",
  "version": "0.0.4",
  "description": "Query Algoila with Apollo",
  "main": "./lib/index.js",
  "repository": "git@github.com:janowsiany/apollo-link-algolia.git",
  "author": "Jan Owsiany <jan.owsiany@gmail.com>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "algolia",
    "apollo",
    "apollo-link",
    "graphql"
  ],
  "scripts": {
    "prepublish": "babel src --out-dir lib --ignore '**/__tests__'",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "dependencies": {
    "algoliasearch-helper": "^2.24.0",
    "apollo-link": "^1.2.1",
    "apollo-utilities": "^1.0.11",
    "graphql": "^0.13.2",
    "graphql-anywhere": "^4.1.8"
  },
  "peerDependencies": {
    "algoliasearch": "^3.26.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.1",
    "husky": "^0.14.3",
    "jest": "^23.2.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.6"
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "prettier --write --config ./.prettierrc",
      "git add"
    ]
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.js"
    ]
  }
